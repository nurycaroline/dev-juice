{
  "name": "dev-juice",
  "displayName": "Dev Juice",
  "description": "Conjunto de ferramentas úteis para desenvolvedores incluindo conversores, formatadores, geradores e validadores",
  "version": "0.0.6",
  "publisher": "NuryStudio",
  "author": "Nurielly Caroline Brizola",
  "license": "MIT",
  "homepage": "https://github.com/nurycaroline/dev-juice",
  "repository": {
    "type": "git",
    "url": "https://github.com/nurycaroline/dev-juice.git"
  },
  "bugs": {
    "url": "https://github.com/nurycaroline/dev-juice/issues"
  },
  "keywords": [
    "developer tools",
    "converter",
    "formatter",
    "generator",
    "validator",
    "utility",
    "CNPJ",
    "CPF",
    "QR Code"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:devJuiceExplorer"
  ],
  "icon": "icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dev-juice",
          "title": "Dev Juice",
          "icon": "./icon.svg"
        }
      ]
    },
    "views": {
      "dev-juice": [
        {
          "id": "devJuiceExplorer",
          "name": "Dev Tools",
          "icon": "./icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "dev-juice.ansiFormatter",
        "title": "Formatador de Logs ANSI",
        "icon": "$(terminal)"
      },
      {
        "command": "dev-juice.ansiFormatterProcessSelection",
        "title": "Dev Juice: Remover Códigos ANSI da Seleção",
        "category": "Editor"
      },
      {
        "command": "dev-juice.base64Encoder",
        "title": "Codificador Base64",
        "icon": "$(lock)"
      },
      {
        "command": "dev-juice.colorConverter",
        "title": "Conversor de Cores",
        "icon": "$(color-mode)"
      },
      {
        "command": "dev-juice.dateCalculator",
        "title": "Calculadora de Data",
        "icon": "$(calendar)"
      },
      {
        "command": "dev-juice.emailValidator",
        "title": "Validador de Email",
        "icon": "$(mail)"
      },
      {
        "command": "dev-juice.formatJson",
        "title": "Formatador JSON",
        "icon": "$(json)"
      },
      {
        "command": "dev-juice.formatTextAlternatingCase",
        "title": "Dev Juice: Formatar para aLtErNaTiNg cAsE",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextCamelCase",
        "title": "Dev Juice: Formatar para camelCase",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextCapitalizedCase",
        "title": "Dev Juice: Formatar para Capitalized Case",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextInverseCase",
        "title": "Dev Juice: Formatar para InVeRsE CaSe",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextKebabCase",
        "title": "Dev Juice: Formatar para kebab-case",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextLowerCase",
        "title": "Dev Juice: Formatar para lower case",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextPascalCase",
        "title": "Dev Juice: Formatar para PascalCase",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextSentenceCase",
        "title": "Dev Juice: Formatar para Sentence case",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextSnakeCase",
        "title": "Dev Juice: Formatar para snake_case",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextUpperCase",
        "title": "Dev Juice: Formatar para UPPER CASE",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextDotNotation",
        "title": "Dev Juice: Formatar para dot.notation",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextParamsStyle",
        "title": "Dev Juice: Formatar para Params: Style",
        "category": "Editor"
      },
      {
        "command": "dev-juice.formatTextPathStyle",
        "title": "Dev Juice: Formatar para path/style",
        "category": "Editor"
      },
      {
        "command": "dev-juice.generateCNPJ",
        "title": "Gerar CNPJ Válido",
        "icon": "$(key)"
      },
      {
        "command": "dev-juice.generateCPF",
        "title": "Gerar CPF Válido",
        "icon": "$(key)"
      },
      {
        "command": "dev-juice.generatePix",
        "title": "Gerar PIX QR Code",
        "icon": "$(credit-card)"
      },
      {
        "command": "dev-juice.generateUUID",
        "title": "Gerar UUID",
        "icon": "$(key)"
      },
      {
        "command": "dev-juice.hashGenerator",
        "title": "Gerador de Hash",
        "icon": "$(key)"
      },
      {
        "command": "dev-juice.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "dev-juice.insertCNPJFormatted",
        "title": "Dev Juice: Inserir CNPJ Formatado",
        "category": "Editor"
      },
      {
        "command": "dev-juice.insertCNPJUnformatted",
        "title": "Dev Juice: Inserir CNPJ Não Formatado",
        "category": "Editor"
      },
      {
        "command": "dev-juice.insertCPFFormatted",
        "title": "Dev Juice: Inserir CPF Formatado",
        "category": "Editor"
      },
      {
        "command": "dev-juice.insertCPFUnformatted",
        "title": "Dev Juice: Inserir CPF Não Formatado",
        "category": "Editor"
      },
      {
        "command": "dev-juice.insertUUIDFormatted",
        "title": "Dev Juice: Inserir UUID Formatado",
        "category": "Editor"
      },
      {
        "command": "dev-juice.insertUUIDUnformatted",
        "title": "Dev Juice: Inserir UUID Não Formatado",
        "category": "Editor"
      },
      {
        "command": "dev-juice.passwordGenerator",
        "title": "Gerador de Senhas",
        "icon": "$(shield)"
      },
      {
        "command": "dev-juice.pixDecoder",
        "title": "PIX QR Code Decoder",
        "icon": "$(zap)"
      },
      {
        "command": "dev-juice.qrReader",
        "title": "QR Code Reader",
        "icon": "$(device-camera)"
      },
      {
        "command": "dev-juice.regexTester",
        "title": "Testador de Regex",
        "icon": "$(search)"
      },
      {
        "command": "dev-juice.textFormatter",
        "title": "Formatação de Texto",
        "icon": "$(symbol-string)"
      },
      {
        "command": "dev-juice.urlEncoder",
        "title": "URL Encoder/Decoder",
        "icon": "$(link)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dev-juice.generateCNPJ",
          "when": "view == devJuiceExplorer",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "vscode-test",
    "audit": "npm audit",
    "prebuild": "npm run audit",
    "build": "npm run lint && npm run compile",
    "security-check": "npm audit && npm run lint"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/qrcode": "^1.5.5",
    "jsqr": "^1.4.0",
    "qrcode": "^1.5.4"
  }
}
